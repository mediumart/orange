{
    "name": "mediumart/orange",
    "type": "library",
    "license": "MIT",
    "keywords": ["orange", "orange-api", "sms", "laravel"],
    "description": "A laravel wrapper/adapter for orange api services.",
    "authors": [
        {
            "name": "Isaac Esso",
            "email": "daprod2009@gmail.com"
        }
    ],
     "require": {
        "php": ">=7.3",
        "mediumart/orange-sms": "~2.0",
        "illuminate/support": "~7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.15 || ^8.5 || ^9.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Mediumart\\Orange\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mediumart\\Orange\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mediumart\\Orange\\OrangeServiceProvider"
            ]
        }
    }
}
